<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/oorangeyellowww"
    tools:context=".AboutUsActivity" >

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/bowlby_one_sc"
        android:text="Welcome to GANADARA K-Training! "
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="11dp"
        android:fontFamily="@font/bowlby_one_sc"
        android:text="Our Mission"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="364dp"
        android:layout_height="88dp"
        android:text="At GANADARA K-Training, we are dedicated to making the journey of learning the Korean language as enjoyable and effective as possible. Our mission is to provide accessible and comprehensive language education tailored to the needs of learners at all levels. "
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="93dp" />
</androidx.constraintlayout.widget.ConstraintLayout>